{
	"name": "frontend-example2",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"projectType": "library",
	"sourceRoot": "libs/frontend-example2/src",
	"prefix": "task-list",
	"targets": {
		"build": {
			"executor": "@nrwl/angular:ng-packagr-lite",
			"outputs": ["{workspaceRoot}/dist/{projectRoot}"],
			"options": {
				"project": "libs/frontend-example2/ng-package.json",
				"tailwindConfig": "libs/frontend-example2/tailwind.config.js"
			},
			"configurations": {
				"production": {
					"tsConfig": "libs/frontend-example2/tsconfig.lib.prod.json"
				},
				"development": {
					"tsConfig": "libs/frontend-example2/tsconfig.lib.json"
				}
			},
			"defaultConfiguration": "production"
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "libs/frontend-example2/jest.config.ts",
				"passWithNoTests": true
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": [
					"libs/frontend-example2/**/*.ts",
					"libs/frontend-example2/**/*.html"
				]
			}
		}
	},
	"tags": []
}
