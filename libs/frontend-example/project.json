{
	"name": "frontend-example",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"projectType": "library",
	"sourceRoot": "libs/frontend-example/src",
	"prefix": "task-list",
	"targets": {
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "libs/frontend-example/jest.config.ts",
				"passWithNoTests": true
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": [
					"libs/frontend-example/**/*.ts",
					"libs/frontend-example/**/*.html"
				]
			}
		}
	},
	"tags": []
}
