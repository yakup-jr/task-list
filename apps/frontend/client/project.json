{
	"name": "frontend-client",
	"$schema": "../../../node_modules/nx/schemas/project-schema.json",
	"projectType": "application",
	"sourceRoot": "apps/frontend/client/src",
	"prefix": "task-list",
	"targets": {
		"build": {
			"executor": "@angular-devkit/build-angular:browser",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/apps/frontend-client/browser",
				"index": "apps/frontend/client/src/index.html",
				"main": "apps/frontend/client/src/main.browser.ts",
				"polyfills": ["zone.js"],
				"tsConfig": "apps/frontend/client/tsconfig.app.json",
				"inlineStyleLanguage": "scss",
				"assets": [
					"apps/frontend/client/src/favicon.ico",
					"apps/frontend/client/src/assets"
				],
				"styles": [
					"@angular/material/prebuilt-themes/deeppurple-amber.css",
					"apps/frontend/client/src/styles.scss"
				],
				"scripts": []
			},
			"configurations": {
				"production": {
					"budgets": [
						{
							"type": "initial",
							"maximumWarning": "500kb",
							"maximumError": "1mb"
						},
						{
							"type": "anyComponentStyle",
							"maximumWarning": "2kb",
							"maximumError": "4kb"
						}
					],
					"outputHashing": "all"
				},
				"development": {
					"buildOptimizer": false,
					"optimization": false,
					"vendorChunk": true,
					"extractLicenses": false,
					"sourceMap": true,
					"namedChunks": true
				}
			},
			"defaultConfiguration": "production"
		},
		"serve": {
			"executor": "@angular-devkit/build-angular:dev-server",
			"configurations": {
				"production": {
					"browserTarget": "frontend-client:build:production",
					"port": 4201
				},
				"development": {
					"browserTarget": "frontend-client:build:development",
					"port": 4200
				}
			},
			"defaultConfiguration": "development",
			"options": {
				"proxyConfig": "apps/frontend/client/proxy.conf.json"
			}
		},
		"extract-i18n": {
			"executor": "@angular-devkit/build-angular:extract-i18n",
			"options": {
				"browserTarget": "frontend-client:build"
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": [
					"apps/frontend/client/**/*.ts",
					"apps/frontend/client/**/*.html"
				]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "apps/frontend/client/jest.config.ts",
				"passWithNoTests": true
			}
		},
		"compodoc": {
			"command": "mkdir docs-temp && cp README.md ./docs-temp && cp ./apps/frontend/client/docs/README.md ./ && npx compodoc -p apps/frontend/client/tsconfig.doc.json --theme postmark -r 8085 -d docs/frontend/client/ && rm README.md && cp ./docs-temp/README.md ./ && rm ./docs-temp/README.md && rmdir ./docs-temp"
		},
		"server": {
			"dependsOn": ["build"],
			"executor": "@angular-devkit/build-angular:server",
			"options": {
				"outputPath": "dist/apps/frontend/client/server",
				"main": "apps/frontend/client/server.ts",
				"tsConfig": "apps/frontend/client/tsconfig.server.json"
			},
			"configurations": {
				"production": {
					"outputHashing": "media"
				},
				"development": {
					"optimization": false,
					"sourceMap": true,
					"extractLicenses": false
				}
			},
			"defaultConfiguration": "production"
		},
		"serve-ssr": {
			"executor": "@nguniversal/builders:ssr-dev-server",
			"configurations": {
				"development": {
					"browserTarget": "frontend-client:build:development",
					"serverTarget": "frontend-client:server:development"
				},
				"production": {
					"browserTarget": "frontend-client:build:production",
					"serverTarget": "frontend-client:server:production"
				}
			},
			"defaultConfiguration": "development"
		},
		"prerender": {
			"executor": "@nguniversal/builders:prerender",
			"options": {
				"routes": ["/"]
			},
			"configurations": {
				"development": {
					"browserTarget": "frontend-client:build:development",
					"serverTarget": "frontend-client:server:development"
				},
				"production": {
					"browserTarget": "frontend-client:build:production",
					"serverTarget": "frontend-client:server:production"
				}
			},
			"defaultConfiguration": "production"
		}
	},
	"tags": [
		"scope:frontend-client",
		"scope:frontend-example",
		"scope:frontend-example2",
		"type:app"
	]
}
